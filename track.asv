clc, clearvars, clear all

data = readtable("track.xlsx");

disp(data);

X = data.X;
Y = data.Y;

invalidMask = (X == 0 | Y == 0 | X == 1e20 | Y == 1e20 | X == -1e20 | Y == -1e20);

X(invalidMask) = [];
Y(invalidMask) = [];

figure(1);
plot(X, Y, 'g');
xlabel("X");, ylabel("Y");, title("Track Map"), grid on;
xlim([,X(end)]), ylim([Y(1), Y(end)]);